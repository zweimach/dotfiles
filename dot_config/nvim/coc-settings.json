{
  "coc.preferences.formatOnSaveFiletypes": ["*"],
  "codeLens.enable": true,
  "diagnostic.floatConfig": {
    "maxHeight": 80,
    "maxWidth": 120
  },
  "eslint.autoFixOnSave": true,
  "hover.floatConfig": {
    "maxHeight": 80,
    "maxWidth": 120
  },
  "go.goplsOptions": {
    "gofumpt": true,
    "linksInHover": false,
    "staticcheck": true,
    "semanticTokens": true
  },
  "languageserver": {
    "astro": {
      "command": "astro-ls",
      "args": ["--stdio"],
      "filetypes": ["astro"],
      "rootPatterns": ["astro.config.mjs", "astro.config.js"]
    },
    "dhall": {
      "command": "dhall-lsp-server",
      "filetypes": ["dhall"]
    },
    "elm": {
      "command": "elm-language-server",
      "filetypes": ["elm"],
      "rootPatterns": ["elm.json"]
    },
    "flow": {
      "command": "flow",
      "args": ["lsp"],
      "filetypes": ["javascript", "javascriptreact"],
      "requireRootPattern": true,
      "rootPatterns": [".flowconfig"]
    },
    "gleam": {
      "command": "gleam",
      "args": ["lsp"],
      "filetypes": ["gleam"],
      "rootPatterns": ["gleam.toml"]
    },
    "graphql": {
      "command": "graphql-lsp",
      "args": ["server", "-m", "stream"],
      "filetypes": [
        "graphql",
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact"
      ],
      "requireRootPattern": true,
      "rootPatterns": [".graphqlrc", ".graphqlrc.*", "graphql.config.*"]
    },
    "haskell": {
      "command": "haskell-language-server-wrapper",
      "args": ["--lsp"],
      "filetypes": ["haskell", "lhaskell"],
      "rootPatterns": [
        "*.cabal",
        "cabal.project",
        "stack.yaml",
        "package.yaml",
        "hie.yaml"
      ],
      "initializationOptions": {
        "languageServerHaskell": {
          "formattingProvider": "fourmolu"
        }
      }
    },
    "ocaml": {
      "command": "opam",
      "args": ["config", "exec", "--", "ocamllsp"],
      "filetypes": ["ocaml", "reason"]
    },
    "purescript": {
      "command": "purescript-language-server",
      "args": ["--stdio"],
      "filetypes": ["purescript"],
      "rootPatterns": ["psc-package.json", "spago.dhall"]
    },
    "vala": {
      "command": "vala-language-server",
      "filetypes": ["vala"]
    },
    "zig": {
      "command": "zls",
      "filetypes": ["zig"]
    }
  },
  "prettier.disableLanguages": [],
  "python.formatting.provider": "black",
  "suggest.completionItemKindLabels": {
    "method": "  ",
    "function": "  ",
    "variable": "[]",
    "field": "  ",
    "typeParameter": "<>",
    "constant": "  ",
    "class": " פּ ",
    "interface": " 蘒",
    "struct": "  ",
    "event": "  ",
    "operator": "  ",
    "module": "  ",
    "property": "  ",
    "enum": " 練",
    "reference": "  ",
    "keyword": "  ",
    "file": "  ",
    "folder": " ﱮ ",
    "color": "  ",
    "unit": " 塞 ",
    "snippet": "  ",
    "text": "  ",
    "constructor": "  ",
    "value": "  ",
    "enumMember": "  "
  },
  "tailwindCSS.experimental.classRegex": [
    "\\bclass[\\s(<|]+\"([^\"]*)\"",
    "\\bclass[\\s(]+\"[^\"]*\"[\\s+]+\"([^\"]*)\"",
    "\\bclass[\\s<|]+\"[^\"]*\"\\s*\\+{2}\\s*\"([^\"]*)\"",
    "\\bclass[\\s<|]+\"[^\"]*\"\\s*\\+{2}\\s*\"[^\"]*\"\\s*\\+{2}\\s*\"([^\"]*)\"",
    "\\bclass[\\s<|]+\"[^\"]*\"\\s*\\+{2}\\s*\"[^\"]*\"\\s*\\+{2}\\s*\"[^\"]*\"\\s*\\+{2}\\s*\"([^\"]*)\"",
    "\\bclassList[\\s\\[\\(]+\"([^\"]*)\"",
    "\\bclassList[\\s\\[\\(]+\"[^\"]*\",\\s[^\\)]+\\)[\\s\\[\\(,]+\"([^\"]*)\"",
    "\\bclassList[\\s\\[\\(]+\"[^\"]*\",\\s[^\\)]+\\)[\\s\\[\\(,]+\"[^\"]*\",\\s[^\\)]+\\)[\\s\\[\\(,]+\"([^\"]*)\""
  ],
  "tailwindCSS.includeLanguages": {
    "elm": "html"
  },
  "semanticTokens.filetypes": ["*"],
  "suggest.noselect": true,
  "svelte.enable-ts-plugin": true,
  "volar.formatting.enable": false
}
