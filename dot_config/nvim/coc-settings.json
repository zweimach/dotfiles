{
  "clangd.semanticHighlighting": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "c",
    "clojure",
    "cpp",
    "css",
    "dart",
    "dhall",
    "elm",
    "go",
    "graphql",
    "haskell",
    "html",
    "java",
    "json",
    "jsonc",
    "javascript",
    "javascriptreact",
    "less",
    "markdown",
    "ocaml",
    "php",
    "purescript",
    "python",
    "racket",
    "rescript",
    "rust",
    "scheme",
    "scss",
    "svelte",
    "toml",
    "typescript",
    "typescriptreact",
    "yaml",
    "vue",
    "zig"
  ],
  "codeLens.enable": true,
  "diagnostic.floatConfig": {
    "maxHeight": 64
  },
  "eslint.autoFixOnSave": true,
  "hover.floatConfig": {
    "maxHeight": 64
  },
  "go.goplsOptions": {
    "gofumpt": true,
    "staticcheck": true,
    "semanticTokens": true
  },
  "javascript.implementationsCodeLens.enable": false,
  "javascript.referencesCodeLens.enable": false,
  "languageserver": {
    "clojure": {
      "command": "clojure-lsp",
      "filetypes": ["clojure"],
      "rootPatterns": ["project.clj"],
      "additionalSchemes": ["jar", "zipfile"]
    },
    "dhall": {
      "command": "dhall-lsp-server",
      "filetypes": ["dhall"]
    },
    "elm": {
      "command": "elm-language-server",
      "filetypes": ["elm"],
      "rootPatterns": ["elm.json"]
    },
    "flow": {
      "command": "flow",
      "args": ["lsp"],
      "filetypes": ["javascript", "javascriptreact"],
      "requireRootPattern": true,
      "rootPatterns": [".flowconfig"]
    },
    "graphql": {
      "command": "graphql-lsp",
      "args": ["server", "-m", "stream"],
      "filetypes": [
        "graphql",
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact"
      ],
      "requireRootPattern": true,
      "rootPatterns": [".graphqlrc", ".graphqlrc.*", "graphql.config.*"]
    },
    "haskell": {
      "command": "haskell-language-server-wrapper",
      "args": ["--lsp"],
      "filetypes": ["haskell", "lhaskell"],
      "rootPatterns": [
        "*.cabal",
        "cabal.project",
        "stack.yaml",
        "package.yaml",
        "hie.yaml"
      ],
      "initializationOptions": {
        "languageServerHaskell": {
          "formattingProvider": "fourmolu"
        }
      }
    },
    "ocaml": {
      "command": "opam",
      "args": ["config", "exec", "--", "ocamllsp"],
      "filetypes": ["ocaml", "reason"]
    },
    "purescript": {
      "command": "purescript-language-server",
      "args": ["--stdio"],
      "filetypes": ["purescript"],
      "rootPatterns": ["psc-package.json", "spago.dhall"]
    },
    "racket": {
      "command": "racket",
      "args": ["--lib", "racket-langserver"],
      "filetypes": ["racket", "scheme"]
    },
    "rescript": {
      "module": "~/.config/nvim/plugged/vim-rescript/server/out/server.js",
      "args": ["--node-ipc"],
      "filetypes": ["rescript"],
      "rootPatterns": ["bsconfig.json"]
    },
    "zig": {
      "command": "zls",
      "filetypes": ["zig"]
    }
  },
  "prettier.disableLanguages": [],
  "python.formatting.provider": "black",
  "rust-analyzer.highlighting.strings": true,
  "rust-analyzer.lens.methodReferences": false,
  "rust-analyzer.lens.implementations": false,
  "rust-analyzer.lens.references": false,
  "suggest.completionItemKindLabels": {
    "method": "  ",
    "function": "  ",
    "variable": "[]",
    "field": "  ",
    "typeParameter": "<>",
    "constant": "  ",
    "class": " פּ ",
    "interface": " 蘒",
    "struct": "  ",
    "event": "  ",
    "operator": "  ",
    "module": "  ",
    "property": "  ",
    "enum": " 練",
    "reference": "  ",
    "keyword": "  ",
    "file": "  ",
    "folder": " ﱮ ",
    "color": "  ",
    "unit": " 塞 ",
    "snippet": "  ",
    "text": "  ",
    "constructor": "  ",
    "value": "  ",
    "enumMember": "  "
  },
  "tailwindCSS.experimental.showPixelValues": true,
  "tailwindCSS.validate": true,
  "typescript.implementationsCodeLens.enable": false,
  "typescript.referencesCodeLens.enable": false,
  "volar.formatting.enable": false,
  "svelte.enable-ts-plugin": true
}
